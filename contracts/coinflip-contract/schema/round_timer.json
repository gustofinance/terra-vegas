{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "RoundTimer",
  "type": "object",
  "required": [
    "current_round",
    "current_round_start_time",
    "round_duration",
    "stopped"
  ],
  "properties": {
    "current_round": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    },
    "current_round_start_time": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    },
    "round_duration": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    },
    "stopped": {
      "type": "boolean"
    }
  }
}
