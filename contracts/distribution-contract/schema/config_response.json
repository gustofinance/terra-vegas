{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ConfigResponse",
  "type": "object",
  "required": [
    "cw20_token_addr",
    "reward_denom",
    "unbonding_period"
  ],
  "properties": {
    "cw20_token_addr": {
      "type": "string"
    },
    "reward_denom": {
      "type": "string"
    },
    "unbonding_period": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    }
  }
}
